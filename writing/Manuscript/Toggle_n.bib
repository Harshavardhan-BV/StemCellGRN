	file = {ScienceDirect Snapshot:/home/bv/Zotero/storage/27YEZ7K9/S0022519321002320.html:text/html},
}

@article{ma_robustness_2006,
	title = {Robustness and modular design of the {Drosophila} segment polarity network},
